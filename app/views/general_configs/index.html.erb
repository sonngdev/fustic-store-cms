<p id="notice"><%= notice %></p>

<h1>General Configs</h1>

<table>
  <thead>
    <tr>
      <th>Landing placeholder image</th>
      <th>Landing vimeo id</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @general_configs.each do |general_config| %>
      <tr>
        <td><%= image_tag general_config.landing_placeholder_image_url, class: 'configs-placeholder-image' %></td>
        <td><%= general_config.landing_vimeo_id %></td>
        <td><%= general_config.active %></td>
        <td><%= link_to 'Show', general_config %></td>
        <td><%= link_to 'Edit', edit_general_config_path(general_config), data: { turbolinks: false } %></td>
        <td><%= link_to 'Destroy', general_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New General Config', new_general_config_path, data: { turbolinks: false } %>
